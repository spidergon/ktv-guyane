---
import Layout from '@layouts/Layout.astro';
import VideoBlock from '@components/VideoBlock.astro';

// src/pages/videos/[id].astro
const { id } = Astro.params;

export function getStaticPaths() {
  return [
    // Generates: /videos/x8b8dou
    {params: {id: 'x8b8dou'}},
  ];
}
---

<Layout title={`${id} - KTV Guyane`}>
  <VideoBlock>
    <script src="https://geo.dailymotion.com/player/x8vkt.js" data-video={id}></script>
  </VideoBlock>
</Layout>
